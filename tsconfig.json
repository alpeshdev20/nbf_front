{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/components/*": ["components/*"],
      "@/public/*": ["public/*"],
      "@/images/*": ["public/images/*"],
      "@/icons/*": ["public/icons/*"],
      "@/styles/*": ["styles/*"],
      "@/data/*": ["data/*"],
      "@/utils/*": ["utils/*"],
      "@/interfaces/*": ["interfaces/*"],
      "@/providers/*": ["providers/*"],
      "@/ui/*": ["components/ui/*"],
      "@/services/*": ["services/*"],
      "@/utlis/*": ["utlis/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "providers/NextAuthProvider.tss"
  ],
  "exclude": ["node_modules"]
}
